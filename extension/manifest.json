{
  "manifest_version": 3,
  "name": "Free Proxy Extension",
  "version": "1.0.0",
  "description": "Quick and easy proxy management - no login required",
  "permissions": [
    "proxy"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_title": "Free Proxy Extension"
  }
}